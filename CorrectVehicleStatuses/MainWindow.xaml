<Window x:Class="CorrectVehicleStatuses.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CorrectVehicleStatuses"
        mc:Ignorable="d"
        Title="MainWindow" Height="650" Width="1000" Loaded="Window_Loaded">
    <Grid>
        <DataGrid x:Name="dgrResults" HorizontalAlignment="Left" Height="599" Margin="10,10,0,0" VerticalAlignment="Top" Width="771" AlternationCount="1" RowBackground="AliceBlue" AlternatingRowBackground="LightBlue"/>
        <Button x:Name="btnProcess" Content="Process" FontSize="35" HorizontalAlignment="Left" Margin="809,248,0,0" VerticalAlignment="Top" Width="149" Height="84" Click="BtnProcess_Click"/>

    </Grid>
</Window>
